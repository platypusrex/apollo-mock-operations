{
  "name": "apollo-mock-operations",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "clean": "turbo run clean --parallel",
    "clean:deps": "turbo run clean:deps --parallel",
    "clean:all": "turbo run clean:all --parallel",
    "build": "turbo run build",
    "dev": "turbo run dev --scope='@examples/*'",
    "storybook": "turbo run storybook --scope='@examples/with-apollo'",
    "start:all": "turbo run start storybook --parallel",
    "lint": "eslint --cache '{packages,examples}/*/{src,test}/**.{ts,tsx,js,md}'",
    "lint:fix": "eslint '{packages,examples}/*/{src,test}/**.{ts,tsx,js,md}' --fix",
    "format": "prettier --check \"**/*.{ts,tsx,js,md}\"",
    "check:types": "yarn turbo run check:types --scope='@graphql-mock-operations/*'",
    "ci": "yarn lint && turbo run check:types && turbo run build --ignore=\"examples/**/*\"",
    "ci:version": "changeset version",
    "prerelease": "yarn ci",
    "release": "yarn prerelease && changeset publish && git push --follow-tags"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.21",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-custom": "*",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "graphql": "16.6.0",
    "prettier": "latest",
    "turbo": "latest"
  },
  "resolutions": {
    "graphql": "16.6.0",
    "react": "18.2.0"
  },
  "packageManager": "yarn@1.22.17"
}
