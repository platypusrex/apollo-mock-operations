{
  "name": "@examples/with-apollo-nextjs",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn generate --watch\"",
    "dev:mocks": "NEXT_PUBLIC_MOCK_OPERATIONS=enabled next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "generate": "graphql-codegen --config codegen.ts",
    "storybook": "DISABLE_ESLINT_PLUGIN=true start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@examples/common": "*",
    "graphql": "^16.6.0",
    "next": "13.0.5",
    "next-merge-props": "^1.0.1",
    "nextjs-apollo-client": "^0.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@apollo-mock-operations/codegen-plugin": "*",
    "@apollo-mock-operations/core": "*",
    "@apollo-mock-operations/storybook-addon": "*",
    "@graphql-codegen/add": "3.2.3",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-operations": "2.5.12",
    "@next/eslint-plugin-next": "^13.1.6",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "concurrently": "^7.6.0",
    "typescript": "^4.9.3"
  }
}
