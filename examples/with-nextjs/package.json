{
  "name": "@examples/with-nextjs",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"next dev\" \"pnpm run generate --watch\"",
    "dev:mocks": "NEXT_PUBLIC_MOCK_OPERATIONS=enabled next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "generate": "graphql-codegen --config codegen.ts",
    "storybook": "DISABLE_ESLINT_PLUGIN=true start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean:deps": "rimraf node_modules"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@examples/common": "*",
    "graphql": "^16.6.0",
    "next": "13.4.18",
    "next-merge-props": "^1.1.1",
    "nextjs-apollo-client": "^0.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@apollo-mock-operations/cli": "^0.0.1",
    "@apollo-mock-operations/codegen-plugin": "*",
    "@apollo-mock-operations/core": "*",
    "@apollo-mock-operations/storybook-addon": "*",
    "@graphql-codegen/add": "5.0.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@next/eslint-plugin-next": "^13.4.19",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  }
}
