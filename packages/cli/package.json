{
  "name": "@apollo-mock-operations/cli",
  "version": "0.0.1",
  "description": "A CLI for quickly scaffolding and initializing @apollo-mock-operations in your codebase",
  "keywords": [
    "typescript",
    "graphql",
    "apollo",
    "mock",
    "mocking",
    "storybook",
    "cypress",
    "jest",
    "testing",
    "next",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platypusrex/apollo-mock-operations.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "create-apollo-mocks": "bin/create-apollo-mocks"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:cjs": "swc src -d dist --config-file .swcrc -C module.type=commonjs",
    "build": "pnpm run clean && pnpm run build:cjs",
    "clean": "rimraf dist",
    "clean:deps": "rimraf node_modules",
    "check:types": "tsc"
  },
  "author": "Frank Cooke",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^3.0.3",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "cosmiconfig": "^8.2.0",
    "cosmiconfig-typescript-loader": "^5.0.0",
    "execa": "^5.1.1",
    "figlet": "^1.6.0",
    "fs-extra": "^11.1.1",
    "json-to-pretty-yaml": "^1.2.2",
    "listr2": "^6.6.1",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/plugin-helpers": "^5.0.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.78",
    "@types/figlet": "^1.5.6",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.1",
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.1",
    "tslib": "^2.6.2",
    "type-fest": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
