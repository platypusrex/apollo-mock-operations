{
  "overwrite": true,
  "schema": "http://localhost:4000",
  "documents": "src/operations/**/*.ts",
  "generates": {
    "src/gql/": {
      "preset": "client",
      "plugins": [
        "@apollo-mock-operations/codegen-plugin"
      ]
    },
    "src/mocking/introspection.json": {
      "plugins": [
        "@graphql-codegen/introspection"
      ]
    }
  }
}