{
  "name": "@apollo-mock-operations/codegen-plugin",
  "version": "0.0.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/dts/index.d.ts",
  "author": "Frank Cooke",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:cjs": "swc src -d dist/cjs --config-file ../../.swcrc -C module.type=commonjs",
    "build:esm": "swc src -d dist/esm --config-file ../../.swcrc -C module.type=es6",
    "build:ts": "tsc --project tsconfig.type.json",
    "build": "yarn clean && concurrently \"yarn build:cjs\" \"yarn build:esm\" \"yarn build:ts\"",
    "clean": "rimraf dist",
    "clean:deps": "rimraf node_modules",
    "check:types": "tsc"
  },
  "devDependencies": {
    "graphql": "^16.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^2.4.1",
    "@graphql-codegen/visitor-plugin-common": "^2.7.1"
  },
  "peerDependencies": {
    "graphql": "16.*"
  }
}
