{
  "name": "@apollo-mock-operations/core",
  "author": "Frank Cooke",
  "description": "Core module utilized in intercepting and mocking requests for Apollo client",
  "keywords": [
    "typescript",
    "graphql",
    "apollo",
    "mock",
    "mocking",
    "storybook",
    "cypress",
    "jest",
    "testing",
    "next"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platypusrex/apollo-mock-operations.git"
  },
  "license": "MIT",
  "version": "0.2.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/dts/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:cjs": "swc src -d dist/cjs --config-file .swcrc -C module.type=commonjs",
    "build:esm": "swc src -d dist/esm --config-file .swcrc -C module.type=es6",
    "build:ts": "tsc --project tsconfig.type.json",
    "build": "yarn clean && concurrently \"yarn build:cjs\" \"yarn build:esm\" \"yarn build:ts\"",
    "clean": "rimraf dist",
    "clean:deps": "rimraf node_modules",
    "check:types": "tsc",
    "lint": "eslint src/**/*.ts"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.2",
    "@swc/cli": "^0.1.60",
    "@swc/core": "^1.3.32",
    "@swc/plugin-emotion": "^2.5.40",
    "@types/react": "^18.0.26",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@graphql-tools/merge": "^8.2.3",
    "@graphql-tools/mock": "^8.5.2",
    "@graphql-tools/utils": "^8.6.2",
    "deepmerge": "^4.2.2"
  },
  "peerDependencies": {
    "@apollo/client": "3.*",
    "graphql": "16.*",
    "react": "17.* - 18.*"
  }
}
