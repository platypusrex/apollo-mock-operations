{
  "name": "@apollo-mock-operations/storybook-addon",
  "author": "Frank Cooke",
  "description": "Storybook addon for mocked Apollo client operations",
  "keywords": [
    "typescript",
    "graphql",
    "apollo",
    "mock",
    "mocking",
    "storybook",
    "cypress",
    "jest",
    "testing",
    "next"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platypusrex/apollo-mock-operations.git"
  },
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/dts/index.d.ts",
  "files": [
    "dist/**/*",
    "*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:cjs": "swc src -d dist/cjs --config-file ../../.swcrc -C module.type=commonjs",
    "build:esm": "swc src -d dist/esm --config-file ../../.swcrc -C module.type=es6",
    "build:ts": "tsc --project tsconfig.type.json",
    "build": "yarn clean && concurrently \"yarn build:cjs\" \"yarn build:esm\" \"yarn build:ts\"",
    "clean": "rimraf dist",
    "clean:deps": "rimraf node_modules",
    "check:types": "tsc"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.2",
    "@storybook/react": "^6.5.14",
    "@types/react": "^18.0.26",
    "concurrently": "^7.0.0",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@storybook/addons": "^6.5.14",
    "@storybook/components": "^6.5.14"
  },
  "peerDependencies": {
    "@apollo/client": "3.*",
    "@storybook/react": "6.*",
    "react": "17.* - 18.x"
  }
}
